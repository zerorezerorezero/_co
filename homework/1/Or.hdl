/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=nota); // a跟自己Nand=>Not a
    Nand(a=b, b=b, out=notb); // b同上
    Nand(a=nota, b=notb, out=out); // 迪摩根 -(a'b')= a'+b'
}
